{
  "rewrites": [
    {
      "source": "/projects",
      "destination": "/projects/index.html"
    },
    {
      "source": "/projects/(.*)",
      "destination": "/projects/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}